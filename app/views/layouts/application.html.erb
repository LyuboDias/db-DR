<!DOCTYPE html>
<html>
  <head>
    <title>Dr. Biswas</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta property="description" content="Dr. Biswas" />
    <meta property="og:image" content="<%= 'dr-logo.png' %>" />
    <meta property="og:url" content="(THE CURRENT REQUESTED URL)" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="Dr. Biswas" />
    <meta property="og:description" content="Dr. Biswas" />
    <meta property="ns:my_custom_meta" content="a value" />
    <meta name="charset" content="utf-8" />
    <meta name="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload', defer: true %>
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
      <%= favicon_link_tag asset_path('dr-logo.png') %>
  </head>

  <body>
    <%= render 'shared/navbar' %>
    <%= yield %>
    <%= render 'shared/footer' %>
  </body>
</html>
